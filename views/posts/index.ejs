<%- include('../partials/header') %>

<script src="js/fabric.min.js"></script>
		<script src="js/canvas2image.js"></script>
		<script src="js/app.js?0.1.0"></script>
		<style type="text/css">
			@font-face{
				font-family: "KH3 Post";
				src: url(font/font.otf);
			}
			body, html {
			    height: 100%;
			    width: 100%;
			    padding: 0;
			    margin: 0;
				background-image: url("data:image/jpeg;base64,")
			}
			canvas {
				display: block;
				margin: 0 auto;
			}
			#container{
				margin: auto;
			}

			
			h1 {
        color: white;
        text-align: center;
      }
      
     
		</style>

<h1>Display Feed</h1>


<% posts.forEach(post => { %>
  <div class="card">
    <div class="card-header">
      <%= post.postedBy %> <img id="avatarPhoto" width="30" src="<%= post.avatar %>" alt="">
    </div>
    <div class="card-body">
      <h5 class="card-title"><%= post.name %></h5>
      <h6><%= post.createdAt.toLocaleString()%></h6>
      <img src= "<%= post.Posts %>" class="rounded mx-auto d-block">
     <div id = "friends"> <%= post.Likes %> Likes 
      <%= post.Caption %> 
    </div>

  
  
      <form action="posts/<%= post._id  %> ?_method=PUT" method="POST">
        <audio id="audio" src="http://www.soundjay.com/button/beep-07.wav" autoplay="false" ></audio>
        <button type="submit" class="btn btn-success" onclick="playSound();">Like</button>
        <script>
          function playSound() {
                var sound = document.getElementById("audio");
                sound.play();
            }
          </script>
  
      </form>
      <form action="posts/<%= post._id %> ?_method=DELETE" method="POST">
        <button type="submit" class="btn btn-secondary">Delete Post</button>
      </form>

      
      </div>
    </div>
  <% }) %>

  
<%- include('../partials/footer') %>